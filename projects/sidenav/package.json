{
    "name": "@hug/ngx-sidenav",
    "version": "19.0.0",
    "description": "HUG Angular - sidenav component",
    "homepage": "https://github.com/dsi-hug/ngx-components",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "maxouhell (https://github.com/maxouhell)",
        "alexandre-guesnerot (https://github.com/alexandre-guesnerot)"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dsi-hug/ngx-components.git"
    },
    "keywords": [
        "angular",
        "material",
        "material design",
        "components"
    ],
    "sideEffects": false,
    "exports": {
        ".": {
            "scss": "./index.scss",
            "m3-panel": "./styles/m3-panel.scss"
        }
    },
    "scripts": {
        "lint": "eslint . --fix",
        "test": "ng test sidenav",
        "test:ci": "ng test sidenav --watch=false --browsers=ChromeHeadless",
        "build:ng": "ng build sidenav -c=production",
        "build": "nx build:ng @hug/ngx-sidenav --verbose",
        "release": "node -r @swc-node/register ../../scripts/release.ts --projects=@hug/ngx-sidenav --verbose",
        "release:dry-run": "npm run release -- --dry-run"
    },
    "peerDependencies": {
        "@angular/common": ">=19 <20",
        "@angular/core": ">=19 <20",
        "rxjs": ">= 7.4.0"
    },
    "dependencies": {
        "tslib": "^2.7.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
